<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Cmm_BackgroundColor">#202020</Color>

    <Color x:Key="CompUI_Cmm_BackgroundColor">#383838</Color>
    <Color x:Key="CompUI_Cmm_BackgroundOverColor">#3d3d3d</Color>
    <Color x:Key="CompUI_Cmm_BackgroundClickColor">#292929</Color>
    <Color x:Key="CompUI_Cmm_BackgroundDisableColor">#3b3b3b</Color>

    <Color x:Key="CompUI_Cmm_BorderColor">#1d1d1d</Color>
    <Color x:Key="CompUI_Cmm_BorderOverColor">#353535</Color>
    <Color x:Key="CompUI_Cmm_BorderClickColor">#303030</Color>

    <Color x:Key="CompUI_Cmm_TextColor">#ffffff</Color>
    <Color x:Key="CompUI_Cmm_TextClickColor">#919191</Color>
    <Color x:Key="CompUI_Cmm_TextDisableColor">#787878</Color>

    <Color x:Key="CompUI_Checkbtn_BackgroundOnColor">#76b9ed</Color>
    <Color x:Key="CompUI_Checkbtn_BackgroundOnOverColor">#6eaad9</Color>
    <Color x:Key="CompUI_Checkbtn_OnCheckColor">#000000</Color>
    <Color x:Key="CompUI_Checkbtn_BackgroundOffColor">#272727</Color>
    <Color x:Key="CompUI_Checkbtn_BackgroundOffOverColor">#343434</Color>
    <Color x:Key="CompUI_Checkbtn_OffCheckColor">#cecece</Color>


    <SolidColorBrush x:Key="CompUI_Background" Color="#fbfbfb" />
    <SolidColorBrush x:Key="CompUI_BackgroundOver" Color="#f6f6f6" />
    <SolidColorBrush x:Key="CompUI_TextColor" Color="#ffffff" />

    <SolidColorBrush x:Key="CompUI_TextColorClick" Color="DimGray" />
    <SolidColorBrush x:Key="CompUI_BorderColor" Color="#e5e5e5" />
    <SolidColorBrush x:Key="CompUI_BorderColorClick" Color="#dddddd" />
    <Thickness x:Key="CompUI_BorderThickness">1</Thickness>
    <CornerRadius x:Key="CompUI_Cmm_CornerRadius">3</CornerRadius>
</ResourceDictionary>