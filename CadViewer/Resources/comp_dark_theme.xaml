<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Cmm_BackgroundColor">#202020</Color>

    <Color x:Key="CompUI_Cmm_BackgroundColor">#383838</Color>
    <Color x:Key="CompUI_Cmm_BackgroundOverColor">#3d3d3d</Color>
    <Color x:Key="CompUI_Cmm_BackgroundClickColor">#292929</Color>
    <Color x:Key="CompUI_Cmm_BackgroundDisableColor">#3b3b3b</Color>

    <Color x:Key="CompUI_Cmm_BorderColor">#3a3a3a</Color>
    <Color x:Key="CompUI_Cmm_BorderEndColor">#414141</Color>
    <Color x:Key="CompUI_Cmm_BorderOverColor">#353535</Color>
    <Color x:Key="CompUI_Cmm_BorderClickColor">#414141</Color>

    <Color x:Key="CompUI_Cmm_TextColor">#ffffff</Color>
    <Color x:Key="CompUI_Cmm_TextClickColor">#919191</Color>
    <Color x:Key="CompUI_Cmm_TextDisableColor">#787878</Color>
    <Color x:Key="CompUI_ToggleBtn_BackgroundOnColor">#76b9ed</Color>
    <Color x:Key="CompUI_ToggleBtn_BackgroundOnOverColor">#6eaad9</Color>
    <Color x:Key="CompUI_ToggleBtn_ThumbOnColor">#000000</Color>
    <Color x:Key="CompUI_ToggleBtn_BackgroundOffColor">#272727</Color>
    <Color x:Key="CompUI_ToggleBtn_BackgroundOffOverColor">#343434</Color>
    <Color x:Key="CompUI_ToggleBtn_ThumbOffColor">#cecece</Color>
    <Color x:Key="CompUI_ToggleBtn_BorderColor">#a1a1a1</Color>

    <SolidColorBrush x:Key="Cmm_TextColor" Color="{DynamicResource CompUI_Cmm_TextColor}" />
    <Thickness x:Key="CompUI_BorderThickness">1</Thickness>
    <CornerRadius x:Key="CompUI_Cmm_CornerRadius">3</CornerRadius>
</ResourceDictionary>