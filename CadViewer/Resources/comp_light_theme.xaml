    <Color x:Key="CompUI_TextBox_BorderColor">#e6e6e6</Color>
    <Color x:Key="CompUI_TextBox_BorderEndColor">#8d8d8d</Color>
    <Color x:Key="CompUI_TextBox_BorderClickEndColor">#005a9e</Color>
    <Color x:Key="CompUI_TextBox_SuggestionItemBackgroundOverColor">#f0f0f0</Color>

    <Color x:Key="CompUI_Slider_TrackValueRangeColor">#0e5f9c</Color>
    <Color x:Key="CompUI_Slider_TrackBackgroundColor">#8b8b8b</Color>
    <Color x:Key="CompUI_Slider_SilderThumbColor">#1a6ba8</Color>
    <Color x:Key="CompUI_Slider_SilderThumbBackgroundColor">#ffffff</Color>

    <SolidColorBrush x:Key="Cmm_TextColor" Color="{DynamicResource CompUI_Cmm_TextColor}" />
    <Thickness x:Key="CompUI_BorderThickness">1</Thickness>
    <CornerRadius x:Key="CompUI_Cmm_CornerRadius">3</CornerRadius>
