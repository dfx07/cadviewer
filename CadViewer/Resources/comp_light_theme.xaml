<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Cmm_BackgroundColor">#f3f3f3</Color>

    <Color x:Key="CompUI_Cmm_BackgroundColor">#fbfbfb</Color>
    <Color x:Key="CompUI_Cmm_BackgroundOverColor">#f6f6f6</Color>
    <Color x:Key="CompUI_Cmm_BackgroundClickColor">#f8f8f8</Color>
    <Color x:Key="CompUI_Cmm_BackgroundDisableColor">#fbfbfb</Color>

    <Color x:Key="CompUI_Cmm_BorderColor">#d2d2d2</Color>
    <Color x:Key="CompUI_Cmm_BorderEndColor">#a6a6a6</Color>
    <Color x:Key="CompUI_Cmm_BorderOverColor">#99d1ff</Color>
    <Color x:Key="CompUI_Cmm_BorderClickColor">#dddddd</Color>

    <Color x:Key="CompUI_Cmm_TextColor">#000000</Color>
    <Color x:Key="CompUI_Cmm_TextClickColor">#aeaeae</Color>
    <Color x:Key="CompUI_Cmm_TextDisableColor">#aaaaaa</Color>
    <Color x:Key="CompUI_ToggleBtn_BackgroundOnColor">#005a9e</Color>
    <Color x:Key="CompUI_ToggleBtn_BackgroundOnOverColor">#1a6ba8</Color>
    <Color x:Key="CompUI_ToggleBtn_ThumbOnColor">#ffffff</Color>
    <Color x:Key="CompUI_ToggleBtn_BackgroundOffColor">#f6f6f6</Color>
