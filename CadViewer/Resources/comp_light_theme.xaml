<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="CompUI_Cmm_BackgroundColor">#fbfbfb</Color>
    <Color x:Key="CompUI_Cmm_BackgroundOverColor">#f6f6f6</Color>
    <Color x:Key="CompUI_Cmm_BackgroundClickColor">#f8f8f8</Color>

    <Color x:Key="CompUI_Cmm_BorderClickColor">#dddddd</Color>
    <Color x:Key="CompUI_Cmm_BorderColor">#e5e5e5</Color>

    <Color x:Key="CompUI_Cmm_TextColor">#000000</Color>
    <Color x:Key="CompUI_Cmm_TextClickColor">#aeaeae</Color>


    <SolidColorBrush x:Key="CompUI_Background" Color="#fbfbfb" />
    <SolidColorBrush x:Key="CompUI_BackgroundOver" Color="#f6f6f6" />
    <SolidColorBrush x:Key="CompUI_TextColor" Color="#000000" />

    <SolidColorBrush x:Key="CompUI_TextColorClick" Color="DimGray" />
    <SolidColorBrush x:Key="CompUI_BorderColor" Color="#e5e5e5" />
    <SolidColorBrush x:Key="CompUI_BorderColorClick" Color="#dddddd" />
    <Thickness x:Key="CompUI_BorderThickness">1</Thickness>
    <CornerRadius x:Key="CompUI_Cmm_CornerRadius">3</CornerRadius>
</ResourceDictionary>